{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":0,
        "no-unused-expressions":0,
        "no-param-reassign":0,
        "import/named":0,
        "no-restricted-syntax":0,
        "no-async-promise-executor":0,
        "import/no-extraneous-dependencies": 0,
        "no-undef": 0,
        "no-underscore-dangle": 0,
        "object-shorthand": 0,
        "no-var": 0,
        "no-console": 0
    }
}
